<section class="perfil">
    <section class="sectionPerfil">
       
        <div class="perfilInfos">
            <img src={{imagem}} alt="imagem perfil usuario">
            <div>
                <span>
                    <h2>{{userName}}</h2>
                    <a href="/seguir/{{iduser}}">Seguir</a>
                </span>

            </div>
        </div>
        <div class="bio">
            <h3>{{userName}}</h3>
            <p>{{bio}}</p>
        </div>
        <div class="rodapeCabecalho">
            <div>
                <p>{{totalPost}}</p>
                <p>publicações</p>
            </div>
            <div>
                <p>{{totalSeguidores}}</p>
                <p>seguidores</p>
            </div>
            <div>
                <p>{{totalSeguindo}}</p>
                <p>seguindo</p>
            </div>
        </div>
      <ul class="postsPerfil">
        {{#each posts}}
        <li>
            <a href="/post/{{idpost}}">
            <img src={{imagem_post}} alt="imagem Post">
            </a>
        </li>
        {{/each }}
      </ul>




    </section>

</section>
